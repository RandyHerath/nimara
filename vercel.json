{
  "version": 2,
  "builds": [
    {
      "src": "apps/stage-web/package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist"
      }
    }
  ],
  "installCommand": "pnpm install --frozen-lockfile",
  "buildCommand": "pnpm --filter=@proj-airi/stage-web run build",
  "outputDirectory": "dist",
  "rewrites": [
    {
      "source": "/remote-assets/page-external-data/js/script.js",
      "destination": "https://plausible.io/js/script.js"
    },
    {
      "source": "/api/v1/page-external-data/submit",
      "destination": "https://plausible.io/api/event"
    }
  ]
}
